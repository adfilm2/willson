<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape
            android:shape="rectangle"
            >
            <stroke
                android:width = "1.5dp"
                android:color = "#ffb800"
                android:dashWidth="4dp"
                android:dashGap="4dp"
                />

            <solid android:color = "#FFFFFF"/>

            <corners
                android:radius="15dp"
                />
        </shape>
    </item>
</selector>